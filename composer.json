{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "anhskohbo/no-captcha": "^3.4",
        "cartalyst/stripe-laravel": "12.0.*",
        "dusterio/link-preview": "^1.2",
        "eyuva/laravel-whatsapp-btn": "1.0.x-dev",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.6",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5.3",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.3",
        "maatwebsite/excel": "~3.1.0",
        "orchestra/parser": "5.x",
        "razorpay/razorpay": "^2.5",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-robots-middleware": "^1.3",
        "stevebauman/location": "^6.5",
        "unicodeveloper/laravel-paystack": "1.0.*",
        "werneckbh/laravel-qr-code": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "jakub-onderka/php-console-highlighter": "^0.4.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/dusk"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/Http/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
